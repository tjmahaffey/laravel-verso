{
    "name": "tjmahaffey/verso",
    "type": "library",
    "description": "Page manager for Laravel.",
    "keywords": ["verso", "page" "laravel"],
    "homepage": "https://github.com/tjmahaffey/laravel-verso",
    "license": "MIT",
    "authors": [
        {
            "name": "T.J. Mahaffey",
            "email": "tj@tjmahaffey.com",
            "homepage": "http://www.tjmahaffey.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "classmap": [
            "app/Models"
        ],
        "psr-4": {
            "Tjmahaffey\\Verso\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tjmahaffey\\Verso\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}